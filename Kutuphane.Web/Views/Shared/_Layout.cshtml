                                    </ul>
                                </nav>
                            </div>
                        </section>
                    </section>
                </section>
            </section>
        </aside>
        <section id="content">
            <section class="vbox">
                @RenderBody()
            </section>
            <a href="#" class="hide nav-off-screen-block" data-toggle="class:nav-off-screen" data-target="#nav"></a>
            <a id="jq-modal-iframe"></a>
            <!-- Modal (BAŞLANGIÇ) -->
            <a href="#jq-modal-popup1" class="jq-modal-popup1-tetikleyici" data-toggle="modal"></a>
            <div class="modal fade" id="jq-modal-popup1">
